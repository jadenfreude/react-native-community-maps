{
  "expo": {
    "name": "YourAppName",
    "slug": "your-app-slug",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "your.bundle.id",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Upload pictures of what you find",
        "NSLocationWhenInUseUsageDescription": "Tell users where you took the picture"
      }
    },
    "android": {
      "package": "your.package.id"
    }

  }
}